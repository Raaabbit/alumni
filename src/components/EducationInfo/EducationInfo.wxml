<view class="{{border ? 'border-t' : ''}} education-item">
  <view class="h3 text-title school">{{data.school}}</view>
  <view class="flex-align-center education">
    <view class="text-primary h3">{{data.education}}</view>
    <view class="text-muted h5 time" wx:if="{{data.startTime || data.endTime}}">
      {{data.startTime || '未填写'}}级,{{data.college}}
    </view>
  </view>
</view>
